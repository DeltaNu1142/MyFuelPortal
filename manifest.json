{
  "domain": "myfuelportal",
  "name": "MyFuelPortal",
  "version": "1.0.5",
  "documentation": "https://www.myfuelportal.com",
  "requirements": [
    "requests",
    "beautifulsoup4"
  ],
  "codeowners": [],
  "config_flow": true,
  "integration_type": "hub",
  "iot_class": "cloud_polling",
  "energy": {
    "sensors": [
      {
        "entity": "sensor._daily_usage",
        "type": "gas",
        "name": "Propane Daily Usage"
      },
      {
        "entity": "sensor._cumulative_usage",
        "type": "gas",
        "name": "Propane Cumulative Usage"
      }
    ]
  }
}
